<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:c='urn:import:com.google.gwt.visualization.client.visualizations.corechart'>
	<ui:style>
		.panel {
			background-color: ivory;
		}
	</ui:style>

	<g:FlowPanel>
		
		<g:HorizontalPanel horizontalAlignment="ALIGN_LEFT" spacing="10">
			<g:MenuBar ui:field="menu"/>
		</g:HorizontalPanel>
		<g:VerticalPanel ui:field="panel" width="100%"/>		 
	</g:FlowPanel> 
	
</ui:UiBinder>
