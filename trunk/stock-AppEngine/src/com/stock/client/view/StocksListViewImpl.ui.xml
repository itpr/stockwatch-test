<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:c='urn:import:com.google.gwt.user.cellview.client'>
	<ui:style>
		.panel {
			background-color: ivory;
		}
	</ui:style>

	<g:FlowPanel>
		<g:HorizontalPanel horizontalAlignment="ALIGN_LEFT" width="250px" spacing="10">
			<g:Button ui:field="prevButton">Prev</g:Button>
			<g:Label ui:field="listName" width="100">All</g:Label>
			<g:Cell horizontalAlignment="ALIGN_RIGHT">
				<g:Button ui:field="nextButton">Next</g:Button>
			</g:Cell>
		</g:HorizontalPanel>
		
		<c:CellList ui:field="stocksList" styleName="mgwt-List"></c:CellList>
	</g:FlowPanel>
</ui:UiBinder>
